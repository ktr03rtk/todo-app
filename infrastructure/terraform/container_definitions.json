{
  "logConfiguration": {
    "logDriver": "awslogs",
    "secretOptions": null,
    "options": {
      "awslogs-group": "${logs_group}",
      "awslogs-region": "${region}",
      "awslogs-stream-prefix": "ecs"
    }
  },
  "entryPoint": ["${entry_point}"],
  "portMappings": [
    {
      "hostPort": 8080,
      "protocol": "tcp",
      "containerPort": 8080
    }
  ],
  "cpu": ${cpu},
  "memoryReservation": ${memory},
  "image": "${image_arn}",
  "readonlyRootFilesystem": false,
  "name": "${container_name}"
}
